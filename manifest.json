{
  "name": "Supermart",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,

  "content_scripts": [{
    "css": ["custom.css"],
    "js": ["jquery-3.3.1.slim.min.js", "custom.js"],
    "matches": [
      "https://search.ticketmonster.co.kr/search/?keyword=*",
      "http://search.ticketmonster.co.kr/search/?keyword=*",
      "https://search.ticketmonster.co.kr/search?keyword=*",
      "http://search.ticketmonster.co.kr/search?keyword=*"
    ]
  }, {
    "js": ["jquery-3.3.1.slim.min.js", "cart.js"],
    "matches": [
      "https://www.ticketmonster.co.kr/deal/*&put_cart=1&*"
    ]
  }]
}
